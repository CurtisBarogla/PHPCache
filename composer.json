{
	"name" : "ness/cache",
	"description" : "PSR Cache Component - Ness",
	"type" : "library",
	"authors" : [{
			"name" : "CurtisBarogla",
			"email" : "curtis_barogla@outlook.fr",
			"role" : "T.B.",
			"homepage" : "https://github.com/CurtisBarogla"
		}
	],
	"autoload" : {
		"psr-4" : {
			"Ness\\Component\\Cache\\" : "src"
		}
	},
	"autoload-dev" : {
		"psr-4" : {
			"NessTest\\Component\\Cache\\" : "tests"
		},
		"files" : [
			"tests/config/test_config.php"
		]
	},
	"keywords" : [
		"psr-6",
		"cache",
		"psr-16",
		"tags",
		"redis",
		"memcached",
		"filesystem",
		"memory"
	],
	"require" : {
		"psr/cache" : "~1.0",
		"psr/simple-cache" : "^1.0",
		"php" : ">= 7.1",
		"cache/tag-interop" : "~1.0"
	},
	"require-dev" : {
		"mikey179/vfsStream" : "~1.6"
	},
	"suggest" : {
		"psr/log" : "To log cache adapter errors"
	},
	"minimum-stability" : "stable",
	"homepage" : "https://github.com/CurtisBarogla/PSR-Cache",
	"license" : "MIT"
}